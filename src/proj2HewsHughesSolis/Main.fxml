<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<StackPane prefHeight="300.0" prefWidth="400.0" stylesheets="@Main.css" xmlns="http://javafx.com/javafx/8.0.76-ea"
           xmlns:fx="http://javafx.com/fxml/1" fx:controller="proj2HewsHughesSolis.Main">
   <children>
     <MenuBar prefHeight="30.0" prefWidth="500.0" StackPane.alignment="TOP_LEFT">
       <menus>
         <Menu mnemonicParsing="false" text="File">
           <items>
             <MenuItem fx:id="exitMenuItem" mnemonicParsing="false"
                       text="Exit"
                       onAction="#handleExit"
                       accelerator="Meta+Q">
             </MenuItem>
           </items>
         </Menu>
         <Menu mnemonicParsing="false" text="Actions">
           <MenuItem fx:id="exitMenuItem" mnemonicParsing="false"
                     text="Play"
                     onAction="#handlePlayMidi"
                     accelerator="Meta+P">
           </MenuItem>
           <MenuItem fx:id="exitMenuItem" mnemonicParsing="false"
                     text="Stop Playing"
                     onAction="#handleStopMidi"
                     accelerator="Meta+S">
           </MenuItem>
         </Menu>
       </menus>
     </MenuBar>
   </children>
</StackPane>
